pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git branch: 'main', credentialsId: '7c278272-a4a0-4985-9fba-4f62e803807f', url: 'https://github.com/Samar-365/devops6.git'
            }}
                
        stage('Compile') {
            steps {
                bat 'javac helloworld.java'
            }}
        stage('Bye') {
            steps {
                bat 'java helloworld'
            }
        }
    }
}

